@model SchoolProgramme.Models.tblwebUserRegistration

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" href="~/Images/plll.jpg" type="image/x-icon" />
    <title>Survey Registration</title>
