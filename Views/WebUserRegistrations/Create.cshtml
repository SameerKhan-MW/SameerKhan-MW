                return false;
            }
        }
    });

    $('#MobileNumber').keypress(function (e) {
        var arr = [];
        var kk = e.which;
        for (i = 48; i < 58; i++)
            arr.push(i);
        if (!(arr.indexOf(kk) >= 0))
            e.preventDefault();
    });
    $('#MobileNumber').blur(function () {
        if ($("#MobileNumber").val() == "") {
            $.bootstrapGrowl("<i class='fa fa-check-circle' aria-hidden='true'></i> Please enter Mobile Number", {
                type: 'danger',
                align: 'center',
                width: 'auto',
                offset: { from: 'top', amount: 70 }, // 'top', or 'bottom'
                allow_dismiss: true,
                delay: 1000
            });
        }
        else {
            var mob = /^[6-9]{1}[0-9]{9}$/;
            var mobile = $("#MobileNumber").val();
            if ($("#MobileNumber").val().length < 10 && mob.test(mobile) == false) {
                $.bootstrapGrowl("<i class='fa fa-check-circle' aria-hidden='true'></i> Please enter valid Mobile Number", {
                    type: 'danger',
