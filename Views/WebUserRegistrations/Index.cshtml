@model IEnumerable<SchoolProgramme.Models.tblwebUserRegistration>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserLevel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MobileNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Block)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Class)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Designation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.District)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UDISE)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Block.BlockName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Class.ClassID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Designation.RoleID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.District.District)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender.GenderID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State.StateName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UDISE.UDISE)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UserRegistrationID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UserRegistrationID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UserRegistrationID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
